{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "asmit/resized-column": "^1.1",
        "binarybuilds/filament-failed-jobs": "^1.1",
        "cmsmaxinc/filament-error-pages": "^2.0",
        "deldius/filament-user-field": "^1.0",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.1",
        "gboquizosanchez/filament-log-viewer": "^2.2",
        "hugomyb/filament-error-mailer": "^4.0",
        "jacobtims/filament-logger": "^1.0",
        "jaocero/radio-deck": "^2.0",
        "kenepa/resource-lock": "^4.0",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "livewire/flux": "^2.1.1",
        "livewire/volt": "^1.7.0",
        "mallardduck/blade-lucide-icons": "^1.23",
        "malzariey/filament-daterangepicker-filter": "^4.0",
        "marcelweidum/filament-expiration-notice": "^1.0",
        "propaganistas/laravel-phone": "^6.0",
        "pxlrbt/filament-activity-log": "^2.0",
        "pxlrbt/filament-excel": "^3.2",
        "schmeits/filament-character-counter": "4.0",
        "sheaf/cli": "^1.3",
        "shuvroroy/filament-spatie-laravel-backup": "^3.1",
        "shuvroroy/filament-spatie-laravel-health": "^3.1",
        "solution-forest/filament-panzoom": "^1.4",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-medialibrary": "^11.15",
        "spatie/laravel-one-time-passwords": "^1.0",
        "spatie/livewire-filepond": "^1.5",
        "webbingbrasil/filament-copyactions": "^4.0",
        "wire-elements/modal": "^3.0",
        "wireui/heroicons": "^2.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "lint": [
            "./vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
